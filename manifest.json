{

    "description": "TimeRun will keep you motivated by showing the time passing by constantly, in different ways of your choice",
    "manifest_version": 2,
    "name": "TimeRun",
    "version": "0.0.32",
    "homepage_url": "https://github.com/mrreiha/timerun",

    "applications": {

        "gecko": {

            "id": "{fe415924-dd4b-4689-8c27-4cfd14f1f624}"

        }

    },

    "icons": {

        "16": "icon/timeRun-16.png",
        "24": "icon/timeRun-24.png",
        "48": "icon/timeRun-64.png",
        "64": "icon/timeRun-64.png",
        "128": "icon/timeRun-128.png"

    },

    "options_ui": {

        "browser_style": true,
        "page": "page/pref.html"


    },

    "chrome_url_overrides": {

        "newtab": "page/index.html"

    },

    "permissions": [

        "storage"

    ]

}
